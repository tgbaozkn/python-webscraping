    "import pandas as pd \n",
    "import requests as req\n",
    "import numpy as np \n",
    "import dateutil as pdate\n",
    "import seffaflik\n",
    "from seffaflik.elektrik import uretim\n",
    "from seffaflik.elektrik import tuketim\n",
    "from seffaflik.elektrik.piyasalar import gop\n",
    "from openpyxl import load_workbook\n",
    "import datetime \n",
    "import time\n",
    "from __future__ import unicode_literals\n",
    "x = datetime.datetime.now() \n",
    "today = datetime.date.today() \n",
    "tomorrow = today + datetime.timedelta(days = 1)  \n",
    "tomorrow = str(tomorrow)\n",
    "yesterday = today - datetime.timedelta(days = 1)  \n",
    "yesterday = str(yesterday)\n",
    "\n",
    "B=[\"o günün\",\"toplamı\"]\n",
    "L=[\"O GÜNÜN\",\"ORTLM\"]\n",
    "dunuretim = uretim.gerceklesen(baslangic_tarihi=yesterday,bitis_tarihi=yesterday)\n",
    "sutunlar = [\"Doğalgaz\",\"Barajlı\",\"Linyit\",\"Akarsu\",\"İthal Kömür\",\"Rüzgar\",\"Güneş\",\"Fuel Oil\",\"Jeo Termal\",\"Asfaltit Kömür\",\"Taş Kömür\",\"Biyokütle\",\"Nafta\",\"LNG\",\"Uluslararası\",\"Toplam\"]\n",
    "sutuns = [\"Güneş\",\"Fuel Oil\",\"Jeo Termal\",\"Asfaltit Kömür\",\"Taş Kömür\",\"Biyokütle\",\"Nafta\",\"LNG\",\"Uluslararası\",\"Toplam\"]\n",
    "\n",
    "for p in range(0,16):\n",
    "    ort = dunuretim[sutunlar[p]].mean()\n",
    "    L.append(ort)\n",
    "    topla = dunuretim[sutunlar[p]].sum()\n",
    "    B.append(topla)\n",
    "dunuretim.loc[1] = L\n",
    "dunuretim.loc[2] = B\n",
    "for s in sutuns :\n",
    "    dunuretim.pop(s),\n",
    "Y=[]\n",
    "r=0\n",
    "while(r<24):\n",
    "    Y.append(str(r))\n",
    "    r=r+1\n",
    "\n",
    "dunuretim = dunuretim.drop(labels=Y,axis=0,inplace=False)\n",
    "path = \"gerceklesen_uretim.xlsx\"\n",
    "writer = pd.ExcelWriter(path, engine = 'xlsxwriter')\n",
    "workbook=writer.book\n",
    "worksheet=workbook.add_worksheet('BÜTÜN VERİLER')\n",
    "writer.sheets['BÜTÜN VERİLER'] = worksheet\n",
    "dunuretim.to_excel(writer,sheet_name = \"BÜTÜN VERİLER\",startrow=0 , startcol=0)\n",
    "##################################################################\n",
    "santraller = [\"tekirdaga\",\"tekirdagb\",\"esenyurt\",\"ista\",\"istb\",\"bursa\"]\n",
    "santral = [\"tekirdaga\",\"tekirdagb\",\"esenyurt\",\"ista\",\"istb\",\"bursa\"]\n",
    "idler= [1112,1224,1123,1230,638,687]\n",
    "santraller[0]= uretim.gerceklesen(baslangic_tarihi=yesterday,bitis_tarihi=yesterday ,santral_id=idler[0])\n",
    "santraller[0]=santraller[0]['Doğalgaz'].to_frame()\n",
    "santraller[0]=santraller[0].rename(columns={'Doğalgaz':santral[0]})\n",
    "\n",
    "A=[]\n",
    "\n",
    "for i in range (1,6):\n",
    "        \n",
    "        santraller[i]=uretim.gerceklesen(baslangic_tarihi=yesterday,bitis_tarihi=yesterday ,santral_id=idler[i])\n",
    "        santraller[i]=santraller[i]['Doğalgaz'].to_frame()\n",
    "        santraller[i] = santraller[i].rename(columns = {'Doğalgaz': santral[i]})\n",
    "        santraller[0]= pd.concat([santraller[0],santraller[i]], axis=1)\n",
    "for j in range(0,6):\n",
    "    mean = santraller[j][santral[j]].mean()\n",
    "    A.append(mean)\n",
    "santraller[0].loc[\"ORTALAMA\"] = A\n",
    "santraller[0] = santraller[0].drop(labels=Y,axis=0,inplace=False)\n",
    "\n",
    "santraller[0].to_excel(writer,sheet_name = \"BÜTÜN VERİLER\",startrow=5 , startcol=0)\n",
    "##################################################################\n",
    "ayingunkiuretimi = uretim.gerceklesen(baslangic_tarihi=yesterday,bitis_tarihi=yesterday)\n",
    "G=[\"ayın o günü\",\"toplam\"]\n",
    "for g in range(0,16):\n",
    "    toplam = ayingunkiuretimi[sutunlar[g]].sum() \n",
    "    G.append(toplam)\n",
    "ayingunkiuretimi.loc[\"toplam\"] = G\n",
    "kaldir2=[\"Saat\",\"Nafta\",\"LNG\",\"Uluslararası\"]\n",
    "for k2 in kaldir2:\n",
    "    ayingunkiuretimi.pop(k2)\n",
    "ayingunkiuretimi[-1:].to_excel(writer,sheet_name = \"BÜTÜN VERİLER\",startrow=23 , startcol=0)\n",
    "gf=ayingunkiuretimi[-1:].set_index('Tarih')\n",
    "gf = gf.div(gf['Toplam'],axis=0)*100\n",
    "gf=gf.reset_index()\n",
    "gf = gf.rename(columns= {'Tarih': 'Yüzde Değerler'})\n",
    "gf.to_excel(writer,sheet_name = \"BÜTÜN VERİLER\",startrow=27 , startcol=0)\n",
    "##################################################################\n",
    "ekimayiuretim = uretim.gerceklesen(baslangic_tarihi=\"2021-10-01\",bitis_tarihi=yesterday)\n",
    "T=[\"ekim ayı\",\"kum\"]\n",
    "    \n",
    "for o in range(0,16):\n",
    "    top = ekimayiuretim[sutunlar[o]].sum()\n",
    "    T.append(top)\n",
    "ekimayiuretim.loc[\"KUMULATIF\"] = T\n",
    "kaldir1=[\"Saat\",\"Nafta\",\"LNG\",\"Uluslararası\"]\n",
    "for k1 in kaldir1:\n",
    "    ekimayiuretim.pop(k1)\n",
    "    \n",
    "\n",
    "df = ekimayiuretim[-1:].set_index('Tarih')\n",
    "df = df.div(df['Toplam'],axis=0)*100\n",
    "df=df.reset_index()\n",
    "df = df.rename(columns= {'Tarih': 'Yüzde Değerler'})\n",
    "ekimayiuretim[-1:].to_excel(writer,sheet_name = \"BÜTÜN VERİLER\",startrow=9 , startcol=0)\n",
    "df.to_excel(writer,sheet_name = \"BÜTÜN VERİLER\",startrow=12 , startcol=0)\n",
    "##################################################################\n",
    "\n",
    "gerceklesentuketim = tuketim.gerceklesen(baslangic_tarihi=yesterday,bitis_tarihi=yesterday)\n",
    "Toplam = gerceklesentuketim[\"Tüketim\"].sum()\n",
    "gerceklesentuketim.loc[\"TOPLAM\"] = Toplam\n",
    "gerceklesentuketim = gerceklesentuketim.rename(columns= {'Tarih': yesterday})\n",
    "\n",
    "gerceklesentuketim[-1:].to_excel(writer,sheet_name = \"BÜTÜN VERİLER\",startrow=15 , startcol=0)\n",
    "ani_puant = gerceklesentuketim.loc[gerceklesentuketim[\"Tüketim\"][:24:].idxmax()]\n",
    "ani_puant.to_excel(writer,sheet_name = \"BÜTÜN VERİLER\",startrow=18 , startcol=0)\n",
    "\n",
    "\n",
    "##################################################################\n",
    "kptf=gop.ptf(baslangic_tarihi=yesterday,bitis_tarihi=yesterday)\n",
    "ORT=kptf[\"PTF\"].mean()\n",
    "kptf.loc[\"PTF ort\"] = ORT\n",
    "kptf[-1:].to_excel(writer,sheet_name = \"BÜTÜN VERİLER\",startrow=31 , startcol=0)\n",
    "\n",
    "\n",
    "writer.save()"
   ]
